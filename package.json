{
  "name": "agent-kit",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build-docker": "turbo run build-docker",
    "cleanup-docker": "docker rmi -f $(docker images agent-kit/frontend --format \"{{.Repository}}:{{.Tag}}\" | grep -v \"latest\" | xargs) && docker rmi -f $(docker images agent-kit/backend --format \"{{.Repository}}:{{.Tag}}\" | grep -v \"latest\" | xargs)",
    "dev": "turbo run dev",
    "drizzle-kit-push": "turbo run drizzle-kit-push -F @agent-kit/backend",
    "drizzle-kit-generate": "turbo run drizzle-kit-generate -F @agent-kit/backend",
    "drizzle-kit-migrate": "turbo run drizzle-kit-migrate -F @agent-kit/backend",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa",
  "engines": {
    "node": ">=18"
  }
}
