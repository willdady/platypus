    "ai": "^5.0.106",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.7",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "drizzle-kit": "^0.31.7"
